{
  "extends": "expo-module-scripts/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "module": "Node16",
    "moduleResolution": "node16",
    "target": "es2022",
    "customConditions": ["my-condition"],
    "jsx": "react-native",
    "strict": true,
    "noEmit": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
